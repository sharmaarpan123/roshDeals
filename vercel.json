{
    "version": 2,
    "builds": [
        {
            "src": "dist/server/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.**)",
            "dest": "dist/server/server.js"
        }
    ],
    "outputDirectory": "dist"
}
