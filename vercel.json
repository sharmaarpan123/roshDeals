{
    "version": 2,
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 10
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/server.js"
        }
    ]
}
